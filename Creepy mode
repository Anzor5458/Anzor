-- froger 
coroutine.wrap(function()
    while true do
        wait(math.random(80,120))
        game.ReplicatedStorage.GameData.LatestRoom.Changed:Wait()
        wait(2)
loadstring(game:HttpGet('https://raw.githubusercontent.com/Anzor5458/Nope-/refs/heads/main/Pressure'))()
    end
end)()

